{
  "metadata": {
    "displayName": "Metadata",
    "attributes": [
      {
        "key": "title",
        "displayName": "title",
        "tag": "<title>",
        "description": "Defines the document's title that is shown in a browser's title bar or a page's tab"
      },
      {
        "key": "description",
        "displayName": "description",
        "tag": "<meta name=\"description\">",
        "description": "A short and accurate summary of the content of the page"
      },
      {
        "key": "viewport",
        "displayName": "viewport",
        "tag": "<meta name=\"viewport\">",
        "description": "Gives hints about the size of the initial size of the viewport"
      },
      {
        "key": "canonical",
        "displayName": "canonical",
        "tag": "<link rel=\"canonical\">",
        "description": "Indicates that another page is representative of the content on the page"
      },
      {
        "key": "ogSiteName",
        "displayName": "og:site_name",
        "tag": "<meta property=\"og:site_name\">",
        "description": "The name which should be displayed for the overall site"
      },
      {
        "key": "ogType",
        "displayName": "og:type",
        "tag": "<meta property=\"og:type\">",
        "description": "The type of the object"
      },
      {
        "key": "ogTitle",
        "displayName": "og:title",
        "tag": "<meta property=\"og:title\">",
        "description": "The title of the object as it should appear within the graph"
      },
      {
        "key": "ogDescription",
        "displayName": "og:description",
        "tag": "<meta property=\"og:description\">",
        "description": "A one to two sentence description of the object"
      },
      {
        "key": "ogUrl",
        "displayName": "og:url",
        "tag": "<meta property=\"og:url\">",
        "description": "The canonical URL of the object that will be used as its permanent ID in the graph"
      },
      {
        "key": "ogImage",
        "displayName": "og:image",
        "tag": "<meta property=\"og:image\">",
        "description": "An image URL which should represent the object within the graph"
      },
      {
        "key": "ogImageAlt",
        "displayName": "og:image:alt",
        "tag": "<meta property=\"og:image:alt\">",
        "description": "A description of what is in the image"
      }      
    ]
  },
  "url": {
    "displayName": "URL",
    "attributes": [
      {
        "key": "https",
        "displayName": "HTTPS",
        "tag": "HTTPS",
        "description": "Privacy and integrity protection."
      },
      {
        "key": "dotgov",
        "displayName": "sTLD",
        "tag": "sTLD",
        "description": "Sponsored top-level domain (.gov / .edu / .mil). "
      },
      {
        "key": "www",
        "displayName": "WWW",
        "tag": "www",
        "description": "Resolves with www and non-www input. "
      }      
    ]
  },
  "sitemap": {
    "displayName": "Sitemap",
    "attributes": [
      {
        "key": "status",
        "displayName": "Status",
        "tag": "Status",
        "description": "The HTTP status of /sitemap.xml is OK."
      },
      {
        "key": "xml",
        "displayName": "XML",
        "tag": "XML",
        "description": "The sitemap file type is XML."
      },
      {
        "key": "completion",
        "displayName": "Complete",
        "tag": "Complete",
        "description": "Every link on the home page is in the sitemap."
      }
    ]
  },
  "robots": {
    "displayName": "Robots",
    "attributes": [
      {
        "key": "valid",
        "displayName": "Valid",
        "tag": "Valid",
        "description": "The site has a valid robots policy."
      },
      {
        "key": "allowed",
        "displayName": "Allowed",
        "tag": "Allowed",
        "description": "The robots policy allows access to browsers and scrapers."
      },
      {
        "key": "sitemap",
        "displayName": "Sitemap",
        "tag": "Sitemap",
        "description": "The robots.txt file points to a sitemap file."
      }
    ]
  },
  "security": {
    "displayName": "Security",
    "attributes": [
      {
        "key": "hsts",
        "displayName": "HSTS",
        "tag": "HSTS",
        "description": "The site automatically upgrades from HTTP to HTTPS."
      },
      {
        "key": "csp",
        "displayName": "CSP",
        "tag": "CSP",
        "description": "The site restricts what can be loaded."
      },
      {
        "key": "xContentTypeOptions",
        "displayName": "X-Content-Type-Options",
        "tag": "X-Content-Type-Options",
        "description": "The site prevents mime type sniffing."
      },
      {
        "key": "securityTxt",
        "displayName": "security.txt",
        "tag": "security.txt",
        "description": "The site has a security.txt file."
      }
    ]
  },
  "performance": {
    "displayName": "Performance",
    "attributes": [
      {
        "key": "ttfb",
        "displayName": "Time to first byte",
        "tag": "TTFB",
        "description": "The application code is received within 0.8 seconds.",
        "docs": "https://docs.scangov.org/time-to-first-byte"
      },
      {
        "key": "fcp",
        "displayName": "First contentful paint",
        "tag": "FCP",
        "description": "The page begins to display within 1.8 seconds.",
        "docs": "https://docs.scangov.org/first-contentful-paint"
      },
      {
        "key": "lcp",
        "displayName": "Largest contentful paint",
        "tag": "LCP",
        "description": "A large part of the application has been displayed within 2.5 seconds.",
        "docs": "https://docs.scangov.org/largest-contentful-paint"
      },
      {
        "key": "cls",
        "displayName": "Cumulative layout shift",
        "tag": "CLS",
        "description": "The page does not move visible elements around during initial display for more than 0.1 seconds at a time.",
        "docs": "https://docs.scangov.org/cumulative-layout-shift"
      },
      {
        "key": "inp",
        "displayName": "Interaction to next paint",
        "tag": "INP",
        "description": "It takes the application less than 0.2 seconds to respond to a user interaction.",
        "docs": "https://docs.scangov.org/interaction-to-next-paint"
      }
    ]
  }
}