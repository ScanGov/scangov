{
  "metadata": {
    "displayName": "Metadata",
    "attributes": [
      {
        "key": "title",
        "displayName": "title",
        "tag": "<title>",
        "description": "Defines the document's title that is shown in a browser's title bar or a page's tab",
        "docs": "https://docs.scangov.org/metadata"
      },
      {
        "key": "description",
        "displayName": "description",
        "tag": "<meta name=\"description\">",
        "description": "A short and accurate summary of the content of the page",
        "docs": "https://docs.scangov.org/metadata"
      },
      {
        "key": "viewport",
        "displayName": "viewport",
        "tag": "<meta name=\"viewport\">",
        "description": "Gives hints about the size of the initial size of the viewport",
        "docs": "https://docs.scangov.org/metadata"
      },
      {
        "key": "canonical",
        "displayName": "canonical",
        "tag": "<link rel=\"canonical\">",
        "description": "Indicates that another page is representative of the content on the page",
        "docs": "https://docs.scangov.org/metadata"
      },
      {
        "key": "ogSiteName",
        "displayName": "og:site_name",
        "tag": "<meta property=\"og:site_name\">",
        "description": "The name which should be displayed for the overall site",
        "docs": "https://docs.scangov.org/open-graph"
      },
      {
        "key": "ogType",
        "displayName": "og:type",
        "tag": "<meta property=\"og:type\">",
        "description": "The type of the object",
        "docs": "https://docs.scangov.org/open-graph"
      },
      {
        "key": "ogTitle",
        "displayName": "og:title",
        "tag": "<meta property=\"og:title\">",
        "description": "The title of the object as it should appear within the graph",
        "docs": "https://docs.scangov.org/open-graph"
      },
      {
        "key": "ogDescription",
        "displayName": "og:description",
        "tag": "<meta property=\"og:description\">",
        "description": "A one to two sentence description of the object",
        "docs": "https://docs.scangov.org/open-graph"
      },
      {
        "key": "ogUrl",
        "displayName": "og:url",
        "tag": "<meta property=\"og:url\">",
        "description": "The canonical URL of the object that will be used as its permanent ID in the graph",
        "docs": "https://docs.scangov.org/open-graph"
      },
      {
        "key": "ogImage",
        "displayName": "og:image",
        "tag": "<meta property=\"og:image\">",
        "description": "An image URL which should represent the object within the graph",
        "docs": "https://docs.scangov.org/open-graph"
      },
      {
        "key": "ogImageAlt",
        "displayName": "og:image:alt",
        "tag": "<meta property=\"og:image:alt\">",
        "description": "A description of what is in the image",
        "docs": "https://docs.scangov.org/open-graph"
      }      
    ]
  },
  "url": {
    "displayName": "URL",
    "attributes": [
      {
        "key": "https",
        "displayName": "HTTPS",
        "tag": "HTTPS",
        "description": "Privacy and integrity protection.",
        "docs": "https://docs.scangov.org/https"
      },
      {
        "key": "dotgov",
        "displayName": "sTLD",
        "tag": "sTLD",
        "description": "Sponsored top-level domain (.gov / .edu / .mil). ",
        "docs": "https://docs.scangov.org/stld"
      },
      {
        "key": "www",
        "displayName": "WWW",
        "tag": "www",
        "description": "Resolves with www and non-www input. ",
        "docs": "https://docs.scangov.org/canonicalization"
      }      
    ]
  },
  "seo": {
    "displayName": "SEO",
    "attributes": [
      {
        "key": "status",
        "displayName": "Status",
        "tag": "Status",
        "description": "The HTTP status of /sitemap.xml is OK.",
        "docs": "https://docs.scangov.org/sitemaps"
      },
      {
        "key": "xml",
        "displayName": "XML",
        "tag": "XML",
        "description": "The sitemap file type is XML.",
        "docs": "https://docs.scangov.org/sitemaps"
      },
      {
        "key": "completion",
        "displayName": "Complete",
        "tag": "Complete",
        "description": "Every link on the home page is in the sitemap.",
        "docs": "https://docs.scangov.org/sitemaps"
      },
      {
        "key": "valid",
        "displayName": "Valid",
        "tag": "Valid",
        "description": "The site has a valid robots policy.",
        "docs": "https://docs.scangov.org/robots-txt"
      },
      {
        "key": "allowed",
        "displayName": "Allowed",
        "tag": "Allowed",
        "description": "The robots policy allows access to browsers and scrapers.",
        "docs": "https://docs.scangov.org/robots-txt"
      },
      {
        "key": "sitemap",
        "displayName": "Sitemap",
        "tag": "Sitemap",
        "description": "The robots.txt file points to a sitemap file.",
        "docs": "https://docs.scangov.org/sitemaps"
      }
    ]
  },
  "security": {
    "displayName": "Security",
    "attributes": [
      {
        "key": "hsts",
        "displayName": "HSTS",
        "tag": "HSTS",
        "description": "The site automatically upgrades from HTTP to HTTPS.",
        "docs": "https://docs.scangov.org/hsts"
      },
      {
        "key": "csp",
        "displayName": "CSP",
        "tag": "CSP",
        "description": "The site restricts what can be loaded.",
        "docs": "https://docs.scangov.org/content-security-policy"
      },
      {
        "key": "xContentTypeOptions",
        "displayName": "X-Content-Type-Options",
        "tag": "X-Content-Type-Options",
        "description": "The site prevents mime type sniffing.",
        "docs": "https://docs.scangov.org/x-content-type-options"
      },
      {
        "key": "securityTxt",
        "displayName": "security.txt",
        "tag": "security.txt",
        "description": "The site has a security.txt file.",
        "docs": "https://docs.scangov.org/security-txt"
      }
    ]
  },
  "performance": {
    "displayName": "Performance",
    "attributes": [
      {
        "key": "ttfb",
        "displayName": "Time to first byte",
        "tag": "TTFB",
        "description": "The application code is received within 0.8 seconds.",
        "docs": "https://docs.scangov.org/time-to-first-byte"
      },
      {
        "key": "fcp",
        "displayName": "First contentful paint",
        "tag": "FCP",
        "description": "The page begins to display within 1.8 seconds.",
        "docs": "https://docs.scangov.org/first-contentful-paint"
      },
      {
        "key": "lcp",
        "displayName": "Largest contentful paint",
        "tag": "LCP",
        "description": "A large part of the application has been displayed within 2.5 seconds.",
        "docs": "https://docs.scangov.org/largest-contentful-paint"
      },
      {
        "key": "cls",
        "displayName": "Cumulative layout shift",
        "tag": "CLS",
        "description": "The page does not move visible elements around during initial display for more than 0.1 seconds at a time.",
        "docs": "https://docs.scangov.org/cumulative-layout-shift"
      },
      {
        "key": "inp",
        "displayName": "Interaction to next paint",
        "tag": "INP",
        "description": "It takes the application less than 0.2 seconds to respond to a user interaction.",
        "docs": "https://docs.scangov.org/interaction-to-next-paint"
      }
    ]
  },
  "accessibility": {
    "displayName": "Accessibility",
    "attributes": [
      {
        "key": "a11y-color-contrast",
        "displayName": "Color contrast",
        "tag": "a11y-color-contrast",
        "description": "Opportunities to improve legibility by ensuring sufficient background, foreground color contrast and that links are discernible without relying on color.",
        "docs": "https://docs.scangov.org/a11y-color-contrast"
      },
      {
        "key": "a11y-best-practices",
        "displayName": "Accessibility best practices",
        "tag": "a11y-best-practices",
        "description": "Improve accessibility by providing sufficient area on touch elements and allowing the viewport sizing to be controlled by the visitor.",
        "docs": "https://docs.scangov.org/a11y-best-practices"
      },
      {
        "key": "a11y-names-labels",
        "displayName": "Names and labels",
        "tag": "a11y-names-labels",
        "description": "Provide the expected tags and attributes so each document can be easily used with a screen reader.",
        "docs": "https://docs.scangov.org/a11y-names-labels"
      },
      {
        "key": "a11y-navigation",
        "displayName": "Navigation",
        "tag": "a11y-navigation",
        "description": "These are opportunities to improve keyboard navigation in your application.",
        "docs": "https://docs.scangov.org/a11y-navigation"
      },
      {
        "key": "a11y-aria",
        "displayName": "ARIA attributes",
        "tag": "a11y-aria",
        "description": "These are opportunities to improve the usage of ARIA in your application which may enhance the experience for users of assistive technology, like a screen reader.",
        "docs": "https://docs.scangov.org/a11y-aria"
      },
      {
        "key": "a11y-language",
        "displayName": "Internationalization and localization",
        "tag": "a11y-language",
        "description": "Follow these recommendations to improve the experience for visitors who may take advantage of translations.",
        "docs": "https://docs.scangov.org/a11y-language"
      },
      {
        "key": "a11y-audio-video",
        "displayName": "Audio and video",
        "tag": "a11y-audio-video",
        "description": "These are opportunities to provide alternative content for audio and video. This may improve the experience for users with hearing or vision impairments.",
        "docs": "https://docs.scangov.org/a11y-audio-video"
      },
      {
        "key": "a11y-tables-lists",
        "displayName": "Tables and lists",
        "tag": "a11y-tables-lists",
        "description": "These are opportunities to improve the experience of reading tabular or list data using assistive technology, like a screen reader.",
        "docs": "https://docs.scangov.org/a11y-tables-lists"
      },
      {
        "key": "hidden",
        "displayName": "Hidden content",
        "tag": "hidden",
        "description": "Follow these recommendations to avoid hiding content from visitors using assistive technology.",
        "docs": "https://docs.scangov.org/hidden"
      }
    ]
  }
}